{
  "name": "@agent-plans/electron",
  "version": "0.2.3",
  "private": true,
  "description": "agent-plans Electron desktop app",
  "author": "tanabe1478",
  "type": "module",
  "main": "./out/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "dev:debug": "electron-vite dev --remoteDebuggingPort 9222",
    "build": "electron-vite build",
    "dist:mac": "pnpm build && electron-builder --mac dmg --arm64 --publish never",
    "dist:mac:unsigned": "pnpm build && CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --mac dmg --arm64 --publish never",
    "preview": "electron-vite preview",
    "test": "node scripts/run-tests.cjs run",
    "test:watch": "node scripts/run-tests.cjs",
    "test:e2e": "pnpm build && playwright test -c playwright.config.ts",
    "lint": "tsc --noEmit",
    "verify:native": "node scripts/verify-native-modules.cjs",
    "verify:runtime": "node scripts/verify-electron-runtime.cjs",
    "postinstall": "node scripts/rebuild-native.cjs"
  },
  "dependencies": {
    "@agent-plans/shared": "workspace:*",
    "@milkdown/crepe": "^7.18.0",
    "@milkdown/ctx": "^7.18.0",
    "@milkdown/kit": "^7.18.0",
    "@tanstack/react-query": "^5.59.0",
    "better-sqlite3": "^12.6.2",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.320.0",
    "open": "^10.0.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-markdown": "^9.0.0",
    "react-router-dom": "^6.28.0",
    "rehype-highlight": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "zod": "^3.22.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@electron/notarize": "^2.5.0",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.3.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.18",
    "clsx": "^2.1.1",
    "electron": "^33.0.0",
    "electron-builder": "^25.0.0",
    "electron-vite": "^2.0.0",
    "jsdom": "^24.1.0",
    "postcss": "^8.4.35",
    "tailwind-merge": "^2.6.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.0",
    "vite": "^5.4.0",
    "vitest": "^2.0.0"
  },
  "build": {
    "appId": "com.tanabe1478.agentplans",
    "productName": "agent-plans",
    "afterSign": "scripts/notarize.cjs",
    "npmRebuild": true,
    "directories": {
      "output": "release"
    },
    "files": [
      "out/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.inherit.plist",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "arm64"
          ]
        }
      ]
    },
    "dmg": {
      "artifactName": "agent-plans-${version}-mac-${arch}.${ext}"
    }
  }
}
